$Resource_type Парикмахерские : permanent
$Parameters
  Вход                          : (нет_клиентов, клиент_прибыл)
  Количество_клиентов_в_очереди : integer
  Занятость_парикмахера         : (Свободен, Занят) = Свободен
  Обслужено_клиентов            : integer
$End

$Resource_type Клиенты : permanent
$Parameters
  Номер              : integer
  Статус             : (нет, в_очереди, в_стрижке) = нет
  Время_прихода      : real = 0.0
  Позиция_в_очереди  : integer = 0
$End

