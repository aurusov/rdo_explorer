$Function ТДП_X : real
$Type = algorithmic
$Parameters
  номер     : such_as Танкеры_для_показа.Номер
  состояние : such_as Танкеры_для_показа.Состояние
  причал    : such_as Танкеры_для_показа.Причал
$Body
  Calculate_if состояние = прибыл        ТДП_X = ТДП_X_Рейд(номер)
  Calculate_if состояние = швартуется    
ТДП_X = ( ( ( Причал_X(причал) - ТДП_X_Рейд(номер) ) * 1.0 * Буксир.ББ ) / 100 ) + ТДП_X_Рейд(номер)
  Calculate_if состояние = пришвартован  ТДП_X = Причал_X(причал)
  Calculate_if состояние = заполняется   ТДП_X = Причал_X(причал)
  Calculate_if состояние = заполнен      ТДП_X = Причал_X(причал)
  Calculate_if состояние = отправляется 
 ТДП_X = ( ( ( 770 - Причал_X(причал) ) * 1.0 * Буксир.ББ ) / 100 ) + Причал_X(причал)
  Calculate_if состояние = отправлен     
ТДП_X = ( ( ( 770 - Причал_X(причал) ) * 1.0 * Буксир.ББ ) / 100 ) + Причал_X(причал)
$End

