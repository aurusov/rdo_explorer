	 ТДП_Y = ( ( ( Причал_Y(причал) - ТДП_Y_Рейд(номер) ) * 1.0 * Буксир.ББ ) / 100 ) + ТДП_Y_Рейд(номер)
  Calculate_if состояние = пришвартован  ТДП_Y = Причал_Y(причал)
  Calculate_if состояние = заполняется   ТДП_Y = Причал_Y(причал)
  Calculate_if состояние = заполнен      ТДП_Y = Причал_Y(причал)
  Calculate_if состояние = отправляется  
ТДП_Y = ( ( ( 200 - Причал_Y(причал) ) * 1.0 * Буксир.ББ ) / 100 ) + Причал_Y(причал) 
  Calculate_if состояние = отправлен     
ТДП_Y = ( ( ( 200 - Причал_Y(причал) ) * 1.0 * Буксир.ББ ) / 100 ) + Причал_Y(причал) 
$End

